{
  "name": "jipangi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_PATH=src next dev -p 8081",
    "start": "NODE_PATH=src next start -p 8081",
    "build": "npm run clean && NODE_PATH=src next build",
    "export": "NODE_PATH=src next export",
    "clean": "rm -rf .next",
    "deploy-dev": "npm install && npm run build && eb deploy",
    "gh-deploy": "npm run build && npm run export && touch out/.nojekyll && git add out/ && git commit -m 'deploy to gh-pages' && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@popmotion/popcorn": "^0.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.10.7",
    "framer-motion": "^1.8.4",
    "lodash": "^4.17.19",
    "next": "9.4.4",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.0.3",
    "next-images": "^1.3.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^4.0.1",
    "next-transpile-modules": "^3.0.2",
    "node-sass": "^4.13.0",
    "popmotion": "^8.7.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-lorem-ipsum": "^1.4.4",
    "react-notion": "^0.7.3",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.0.1",
    "styled-jsx": "^3.2.4",
    "tailwindcss": "^1.4.6"
  }
}
